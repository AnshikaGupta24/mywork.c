#include <stdio.h>

int main()
{
    int t,n;
   
    printf("enter a number of elements of array");
    scanf("%d", &t);
    
    int a[t];
    
    printf("input elements array\n");
    for(int i =0;i<t;i++){
      scanf("%d", &a[i]);    
    }
    
    printf("enter a number to be searched\n");
    scanf("%d", &n);
    
    for(int i=0;i<t;i++)
    {
        if(n==a[i])
        {
            printf("element found!");
              break;
        }
      
    }
    
    return 0;
}
