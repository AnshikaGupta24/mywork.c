#include <stdio.h>

int main()
{
    int a,b;
    char operator;
    printf("enter two operands\n");
    printf("enter an operator");
    scanf("%d %d ", &a, &b);
    scanf("%c", &operator);
    switch(operator)
    {
        case '+':
        a+b;
        printf("%d", a+b);
        break;
        
        case '-':
        a-b;
        printf("%d", a-b);
        break;
        
        case '*':
        a*b;
        printf("%d", a*b);
        break;
        
        case '%':
        a%b;
        printf("%d",a%b);
        break;
        
        default:
        printf("oops! operator not found");
        
    }
   return 0;
}
